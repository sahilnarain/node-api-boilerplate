{
  "env": {
    "node": true,
    "jest": true,
    "commonjs": true,
    "es2022": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "indent": [
            "error",
            2
        ],
    "linebreak-style": [
            "error",
            "unix"
        ],
    "quotes": [
            "error",
            "single"
        ],
    "semi": [
            "error",
            "always"
        ],
    "no-unused-vars": [
          "warn"
        ],
    "no-prototype-builtins": [
          "off"
        ],
    "no-useless-escape": [
          "off"
        ]
  }
}
